    <SpriteAnim name="Inventory.Helm"          spriteSheet="Inventory"   startCoords="3,3" endCoords="3,3" playbackType="Once"/>
    <SpriteAnim name="Inventory.Chest"         spriteSheet="Inventory"   startCoords="0,2" endCoords="0,2" playbackType="Once"/>
    <SpriteAnim name="Inventory.Shoulder"      spriteSheet="Inventory"   startCoords="2,2" endCoords="2,2" playbackType="Once"/>
    <SpriteAnim name="Inventory.Legs"          spriteSheet="Inventory"   startCoords="3,2" endCoords="3,2" playbackType="Once"/>
    <SpriteAnim name="Inventory.Feet"          spriteSheet="Inventory"   startCoords="1,3" endCoords="1,3" playbackType="Once"/>
    <SpriteAnim name="Inventory.Weapon"        spriteSheet="Inventory"   startCoords="4,3" endCoords="4,3" playbackType="Once"/>
